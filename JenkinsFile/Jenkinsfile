pipeline {
	agent any
	tools{
		maven 'mvn3.3.9'
	}
	stages 
	{
		stage ('Initialize') {
            steps {
                sh '''
                    echo "PATH = ${PATH}"
                    echo "M2_HOME = ${M2_HOME}"
                '''
            }
        }
	   stage('Checkout') 
	   {
	   steps {
	   		sh "mvn --version"
	   		//checkout scm   
	   	}
	   }
	   
	   stage('Build') 
	   {  
		steps {
	  		sh "mvn clean install"
	  	}
       }
		
	   stage('Test') 
	   { 
        steps {
            sh 'mvn test' 
        }
       }
	}
}